	<script>
// Sélectionnez tous les liens <a href>
	var links = document.querySelectorAll("a");

// Parcourez tous les liens et ajoutez un gestionnaire d'événements de clic
links.forEach(function(link) {
  link.addEventListener("click", function(event) {
    // Vérifiez si la touche Shift est enfoncée pour ouvrir le lien dans un nouvel onglet
    if (!event.shiftKey) {
      // Empêchez le comportement de lien par défaut
      event.preventDefault();
      
      // Naviguez vers la nouvelle page en utilisant l'attribut href du lien
      var href = this.getAttribute("href");
      history.pushState({}, "", href);
      
      // Rechargez la page de manière discrète
      location.reload();
    }
  });
});
	</script>

