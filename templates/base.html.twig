					const disparition = document.querySelector('.disparition');

					window.addEventListener('scroll', () => {
						const scrollPosition = window.scrollY + window.innerHeight;
						const documentHeight = document.documentElement.scrollHeight;

						if (scrollPosition >= documentHeight) {
							disparition.classList.add('hidden');
						} else {
							disparition.classList.remove('hidden');
						}
					});
				});
			</script>
			<script>
				document.addEventListener('DOMContentLoaded', function () {
					const logo = document.querySelector('.header__logo');

					window.addEventListener('scroll', function () {
						// Calculer la hauteur totale du document
						const documentHeight = document.documentElement.scrollHeight;
						// Calculer la hauteur visible de la fenêtre
						const windowHeight = window.innerHeight;
						// Calculer la position actuelle du scroll
						const scrollPosition = window.scrollY + windowHeight;

						// Vérifier si la position du scroll est au bas de la page
						if (scrollPosition >= documentHeight) {
							logo.classList.add('hide-logo');
						} else {
							logo.classList.remove('hide-logo');
