	<script>
		var refreshing = false;

		function refreshPage() {
			if (!refreshing) {
				refreshing = true;
				setTimeout(() => {
					location.reload();
					refreshing = false;
				}, 1000); // 500 ms = 0.5 seconde de délai
			}
		}

		// Écouteur d'événements pour détecter les changements dans l'historique
		window.addEventListener('popstate', function (event) {
			refreshPage();
		});

		// Pour gérer les changements d'état via pushState et replaceState
		function trackHistoryChanges() {
			var pushState = history.pushState;
			var replaceState = history.replaceState;

			history.pushState = function (state, title, url) {
				pushState.apply(history, arguments);
				refreshPage();
			};

			history.replaceState = function (state, title, url) {
				replaceState.apply(history, arguments);
				refreshPage();
			};
		}

		// Appeler cette fonction pour surveiller les changements d'état
		trackHistoryChanges();


	</script>

